{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/$",
      "status": 308,
      "headers": {
        "Location": "/v1/login"
      }
    },
    {
      "src": "/login.html",
      "dest": "/public/login.html"
    },
    {
      "src": "/vendor/supabase.js",
      "dest": "/public/vendor/supabase.js"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/logo.svg",
      "dest": "/public/logo.svg"
    },
    {
      "src": "/v1/(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}